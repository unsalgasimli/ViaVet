<AnchorPane minHeight="-Infinity" minWidth="-Infinity" stylesheets="@../../resources/app.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.app.staff.StaffHistoryController">
